{
    "manifest_version": 3,
    "name": "Quick Toggle",
    "version": "1.0",
    "description": "Disable/Enable all your Chrome extensions with a click.",
    "options_page": "settings.html",
    "permissions": [
        "management",
        "storage"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "192": "icons/icon192.png",
        "512": "icons/icon512.png"
    }
}